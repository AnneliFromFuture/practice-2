@model IEnumerable<MorePractice.Specializ>

@{
    ViewBag.Title = "Список специализаций";
}

<h2>Список специализаций</h2>

<p>
    @Html.ActionLink("Добавить специализацию", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayName("Специализация")
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Specialization)
            </td>
            <td>
                @Html.ActionLink("Редактировать", "Edit", new { id = item.ID }) |
                @Html.ActionLink("Детали", "Details", new { id = item.ID }) |
                @Html.ActionLink("Удалить", "Delete", new { id = item.ID })
            </td>
        </tr>
    }

</table>